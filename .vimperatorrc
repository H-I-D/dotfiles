"3.16.0 (created: 2017/02/01 00:55:15)

source! /Users/hideaki/.vimperatorrc.local

" vim: set ft=vimperator:
autocmd LocationChange .* :js liberator.modules.modes.passAllKeys=false
autocmd LocationChange localhost :js liberator.modules.modes.passAllKeys=true
autocmd LocationChange .*\.google\.com :js liberator.modules.modes.passAllKeys=true
autocmd LocationChange .*\.youtube\.com :js liberator.modules.modes.passAllKeys=true
noremap j 5<C-e>
noremap k 5<C-y>
set ignorecase
set smartcase
set complete=l
nnoremap s :tabopen<Space>google<Space>
nmap S <C-c>:tabopen<Space>google<Space><C-v><CR>
vmap S <C-c><Esc><Esc>:tabopen<Space>google<Space><C-v><CR>
set hintchars=asdfghjkl
" bookmarks
qmark g https://github.com/vwrs
qmark w https://my.waseda.jp
qmark c https://cnavi.waseda.jp
qmark d https://drive.google.com/drive/u/0/recent
qmark t http://maruta.github.io/timekeeper/#t1=20:00&t2=30:00&t3=99:00&m=院ゼミタイマー
qmark y https://www.youtube.com
qmark s https://github.com/search
